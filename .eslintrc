{
    "env": {
        "browser": true,
        "node": true,
        "commonjs": true
    },
    "extends": "eslint:recommended",
    "parserOptions": {
        "ecmaVersion": 5
    },
    "globals": {
        "angular": false
    },
    "rules": {
        "indent": [
            "error", "tab"
        ],
        "linebreak-style": [
            "error", "windows"
        ],
        "quotes": [
            "error", "single"
        ],
        "semi": [
            "error", "always"
        ],
        "no-extra-parens": [
            "error", "all", {
                "nestedBinaryExpressions": false
            }
        ]
    }
}